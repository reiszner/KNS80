        </enable>
        <input>
            <condition>
                <less-than>
                    <property>instrumentation/kns80/dme/indicated-distance-nm</property>
                    <value>99.5</value>
                </less-than>
            </condition>
            <expression>
                <floor>
                    <sum>
                        <product>
                            <property>instrumentation/kns80/dme/indicated-distance-nm</property>
                            <value>10</value>
                        </product>
                        <value>0.5</value>
                    </sum>
                </floor>
            </expression>
        </input>
        <input>
            <expression>
                <floor>
                    <sum>
                        <property>instrumentation/kns80/dme/indicated-distance-nm</property>
                        <value>0.5</value>
                    </sum>
                </floor>
            </expression>
        </input>
        <input>instrumentation/kns80/dme/indicated-distance-nm</input>
        <output>instrumentation/kns80/displayed-indicated-nm</output>
    </filter>

