            </greater-than>
            <property>instrumentation/kns80/nav/in-range</property>
        </enable>
        <input>
            <expression>
                <product>
                    <cos>
                        <deg2rad>
                            <sum>
                                <property>instrumentation/kns80/used-radial</property>
                                <property>instrumentation/kns80/nav/internal/station-declination</property>
                            </sum>
                        </deg2rad>
                    </cos>
